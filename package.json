{
  "name": "markdownmaster",
  "version": "4.0.0",
  "type": "module",
  "description": "The JavaScript Site Generator, forked from Chris Diana's CMS.js",
  "keywords": [
    "client-side, javascript, site-generator, cms, markdown"
  ],
  "homepage": "https://github.com/cdp1337/markdownmaster",
  "scripts": {
    "compile": "rollup -c",
    "minify": "uglifyjs dist/cms.js -m --comments /^!/ -o dist/cms.min.js && copyfiles --flat dist/cms.min.js examples/js/",
    "build": "npm run compile && npm run minify",
    "pack": "./build_pack.sh",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel serve watch",
    "docs": "jsdoc -c jsdoc.conf",
    "serve": "serve ./examples/",
    "test": "npm run test_js && npm run test_py",
    "test_js": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test_py": "PYTHONPATH=. venv/bin/python -m unittest",
    "prepare": "npm run prepare_husky && npm run prepare_venv && npm run prepare_pip",
    "prepare_husky": "husky install",
    "prepare_venv": "python3 -m venv venv",
    "prepare_pip": "venv/bin/pip3 install -r requirements.txt",
    "lint": "npx eslint src/"
  },
  "main": "dist/cms.js",
  "module": "dist/cms.es.js",
  "browser": "dist/cms.min.js",
  "unpkg": "dist/cms.js",
  "jsdelivr": "dist/cms.js",
  "files": [
    "src",
    "dist/*.js",
    "examples",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cdp1337/markdownmaster.git"
  },
  "author": {
    "name": "Charlie Powell"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdp1337/markdownmaster/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.21.5",
    "@babel/preset-env": "^7.2.0",
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-eslint": "^9.0.4",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/copyfiles": "^2.4.1",
    "@types/uglify-js": "^3.17.1",
    "acorn": ">=6.4.1",
    "babel-jest": "^29.5.0",
    "braces": ">=2.3.1",
    "c8": "^7.13.0",
    "copyfiles": "^2.4.1",
    "eslint-utils": ">=1.4.1",
    "expect": "^29.5.0",
    "husky": "^8.0.3",
    "ini": ">=1.3.6",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^22.0.0",
    "js-yaml": "^4.1.0",
    "jsdoc": "^4.0.2",
    "lodash": ">=4.17.19",
    "marked": ">=4.0.0",
    "minimist": ">=1.2.3",
    "mixin-deep": ">=1.3.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.21.6",
    "rollup-plugin-livereload": "^2.0.5",
    "serialize-javascript": ">=3.1.0",
    "serve": "^14.2.0",
    "set-value": ">=2.0.1",
    "uglify-js": "^3.17.4"
  }
}
